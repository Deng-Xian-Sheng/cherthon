<?xml version="1.0"?>
<package >
  <metadata>
    <id>herthondaily.symbols</id>
    <title>Herthon (Daily build)</title>
    <version>0.0.0.0</version>
    <authors>Herthon Software Foundation</authors>
    <projectUrl>https://www.herthon.org/</projectUrl>
    <description>Contains symbols for the daily build of Herthon.</description>
    <iconUrl>https://www.herthon.org/static/favicon.ico</iconUrl>
    <tags>herthon</tags>
  </metadata>
  <files>
    <file src="herthon*.dll" target="tools" exclude="*_d*" />
    <file src="herthon*.exe" target="tools" exclude="*_d*" />
    <file src="herthon*.pdb" target="tools" exclude="*_d*" />
    <file src="*.pdb" target="tools\DLLs" exclude="herthon*;*_test*;xx*;_freeze*;*_d*;py.*;pyw.*;pyshellext.*" />
    <file src="*.pyd" target="tools\DLLs" exclude="herthon*;*_test*;xx*;_freeze*;*_d*;py.*;pyw.*;pyshellext.*" />
    <file src="libcrypto*.dll;libssl*.dll;sqlite3.dll" target="tools\DLLs" />
    <file src="libcrypto*.pdb;libssl*.pdb;sqlite3.pdb" target="tools\DLLs" />
  </files>
</package>
